{
  "name": "@lenne.tech/cli-plugin-helper",
  "version": "0.0.13",
  "description": "Helper plugin for Gluegun CLI Projects",
  "keywords": [
    "lenne.Tech",
    "cli",
    "gluegun",
    "plugin",
    "extension"
  ],
  "contributors": [
    "Pascal Klesse",
    "Kai Haase <kai.haase@lenne.tech> (http://lenne.tech)"
  ],
  "homepage": "http://lenne.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/cli-plugin-helper"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/cli-plugin-helper/issues"
  },
  "scripts": {
    "build": "npm run lint && npm run clean-build && npm run compile",
    "clean-build": "rimraf ./dist",
    "compile": "tsc -p .",
    "coverage": "jest --coverage",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "lint": "eslint './src/**/*.{ts,js,vue}'",
    "lint:fix": "eslint './src/**/*.{ts,js,vue}' --fix",
    "prepack": "npm run build",
    "preversion": "npm run lint",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@lenne.tech/gluegun-menu": "0.0.7"
  },
  "devDependencies": {
    "@lenne.tech/eslint-config-ts": "2.1.3",
    "@lenne.tech/npm-package-helper": "0.0.12",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "find-file-up": "2.0.1",
    "gluegun": "5.2.0",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "find-file-up": ">=2",
    "gluegun": ">=4"
  },
  "overrides": {
    "apisauce@*": "3.1.1",
    "cross-spawn@*": "7.0.6",
    "ejs@*": "3.1.10",
    "semver@*": "7.7.1",
    "lodash.trim@*": "npm:lodash@4.17.21",
    "lodash.trimend@*": "npm:lodash@4.17.21"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "LICENSE",
    "assets/demo.gif",
    "README.md"
  ]
}
